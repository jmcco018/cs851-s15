var counter = 1,
autoPlay = true,
rotateSpeed = 5000,
numSlides = $('#home-intro > div').length - 1;
function rotate() {
	if($('#home-intro > div:visible').index('#home-intro > div') < numSlides){
		$('#home-intro > div').fadeOut('slow').eq(''+counter+'').fadeIn('slow');
		counter++;
	}else{
		counter = 0;
		$('#home-intro > div').fadeOut('slow').eq(''+counter+'').fadeIn('slow');
		counter++;
	}
}
if(autoPlay){
	run = setInterval(rotate, rotateSpeed);
}
</script>
