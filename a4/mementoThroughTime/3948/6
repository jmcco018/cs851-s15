<script id="profileUserinfoTmpl" type="text/x-jquery-tmpl">
	<div id="user_info">
		<div id="user_info_header">
				{{if showAsPrivateProfile}}
					<h1>${webName}</h1>
				{{else}}
					<h1 fbid="${fbid}" webname="${webName}" class="facebookName">
						{{if fullName}}
							{{if hasExplicitContentFilterEnabled}}
								${fullName}
							{{else}}
								${fullNameRaw}
							{{/if}}
						{{else}}
							${webName}
						{{/if}}
					</h1>
				{{/if}}
		</div>

        {{if show_details}}

		    {{if privateProfile}}
			    <div class="profile_permissions profile_private">Private Profile</div>
		    {{else}}
			    <div class="profile_permissions profile_public">Public Profile</div>
		    {{/if}}
		    <ul class="clearfix">
	    	    {{if city}}
					<li><span>Hometown:
						<span class="user_city"><a href="/content/profileSearch/city/{{= escape($data.cityRaw) }}">{{if hasExplicitContentFilterEnabled}}${city}{{else}}${cityRaw}{{/if}}</a></span></span>
					</li>
				{{/if}}
			    {{if school}}
					<li>
						{{if city}} |&nbsp; &nbsp; {{/if}}
						<span>School:
						<span class="user_school"><a href="/content/profileSearch/school/{{= escape($data.schoolRaw) }}">{{if hasExplicitContentFilterEnabled}}${school}{{else}}${schoolRaw}{{/if}}</a></span></span>
						
					</li>
				{{/if}}
			    {{if placeOfWork}}
					<li>
						{{if city || school}} |&nbsp; &nbsp; {{/if}}
						<span>Employer:
						<span class="user_employer"><a href="/content/profileSearch/work/{{= escape($data.placeOfWorkRaw) }}">{{if hasExplicitContentFilterEnabled}}${placeOfWork}{{else}}${placeOfWorkRaw}{{/if}}</a></span></span>
					</li>
				{{/if}}
		    </ul>
        {{/if}}
	</div>

    {{if show_details}}
	    {{if bio}}
		    <div class="user_message" id="bio_text">{{if hasExplicitContentFilterEnabled}}${bio}{{else}}${bioRaw}{{/if}}</div>
	    {{else}}
		    <div class="user_message" id="bio_text">This is your space to write something about yourself</div>
	    {{/if}}

	    {{if is_owner}}
		    <div class="user_message"><a href="#" id="edit_profile_link">Edit profile</a></div>
	    {{/if}}
    {{/if}}

</script>

<script id="profileUserinfoFormTmpl" type="text/x-jquery-tmpl">
	<form>
		<input type="text" id="fullname" maxlength='45' value="{{if fullNameRaw}}${fullNameRaw}{{else}}${webName}{{/if}}">
		<ul>
			<li>
				<div class="privacy_container clearfix">
					{{if privateProfile}}
						<div id="privacy_menu" class="profile_permissions profile_private">Private Profile
					{{else}}
						<div id="privacy_menu" class="profile_permissions profile_public">Public Profile
					{{/if}}
					<span class="dropdownarrow"></span></div>
					<a target="_blank" href="/web/20121117171028/http://help.pandora.com/customer/portal/articles/24663-profile-privacy">Tell me more about privacy</a>
				</div>

				<div id="privacy_menu_dd" class="dropdownmenu">
					<ul>
						<li><a href="#" class="profile_permissions profile_private">Private Profile</a></li>
						<li><a href="#" class="profile_permissions profile_public">Public Profile</a></li>
					</ul>
				</div>
				<input type="hidden" id="privacy" value="{{if privateProfile}}private{{else}}public{{/if}}">
			</li>
			<li>
				<label for="hometown">Hometown</label>
				<input id="hometown" maxlength='45' value="{{if cityRaw}}${cityRaw}{{/if}}">
			</li>
			<li>
				<label for="school">School</label>
				<input id="school" maxlength='45' value="{{if schoolRaw}}${schoolRaw}{{/if}}">
			</li>
			<li>
				<label for="employer">Employer</label>
				<input id="employer" maxlength='45' value="{{if placeOfWorkRaw}}${placeOfWorkRaw}{{/if}}">
			</li>
		</ul>

		<textarea id="bio">{{if bioRaw}}${bioRaw}{{/if}}</textarea>
		<button id="save_profile"></button>
	</form>
</script>

    
