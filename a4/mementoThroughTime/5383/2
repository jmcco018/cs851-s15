<script type="text/javascript">
//<![CDATA[
	// Gets a list of items for the circular carousel
	var _promoItems = $('#promo-slider > li');
	
	// Adds a new item if it is not already in the carousel
	function _itemVisibleInCallback(carousel, item, i, state, evt){
		var idx = carousel.index(i, _promoItems.length);
		carousel.add(i,$(_promoItems[idx - 1]).html());
	};
	
	// Removes excess items from the carousel 
	function _itemVisibleOutCallback(carousel, item, i, state, evt){
		carousel.remove(i);
		carousel.startAuto();
	};
	
	function _initCallback(carousel){
		// Stop the auto scrolling if a user hits the next or previous buttons.
		carousel.buttonNext.bind('click', function() { carousel.startAuto(0); });
	
		carousel.buttonPrev.bind('click', function() { carousel.startAuto(0); });
	
		// Pause autoscrolling if the user moves with the cursor over the clip.
		carousel.clip.hover(function() { carousel.stopAuto(); }, function() {
			carousel.startAuto();
		});
	};
	
	// INIT
	$('#promo-slider').jcarousel({
		scroll: 1
		,wrap: 'circular'
		,auto: 5
		,initCallback: _initCallback
		,itemVisibleInCallback: {onBeforeAnimation: _itemVisibleInCallback}
		,itemVisibleOutCallback: {onAfterAnimation: _itemVisibleOutCallback}
	});
//]]>
</script> 
