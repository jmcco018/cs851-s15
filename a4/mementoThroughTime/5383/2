//<![CDATA[
	$(document).ready(function(){
		cur_random = Math.random();
		$('#topmenu').load('/users/header.cfm?cb='+cur_random);
		
		// Remove search default text on focus
		$('#s_sitesearch').focus(function(){
			if(this.value == 'Search/Keyword'){
				this.value = '';
			}
			$(this).css({color:'#000'});
		});
		
		// Remove search default text on blur
		$('#s_sitesearch').blur(function(){
			if(this.value == ''){
				this.value = 'Search/Keyword';
				$(this).css({color:'#CCC'});
			}
		});
	
	});
	
//]]>
