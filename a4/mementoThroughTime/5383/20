  ul = $('<ul id="weather_alerts"></ul>');
  if(weather.length > 0){
    $.each(weather, function(i, item){
      counties = "";
      $.each(item.COUNTIES, function(j, county){
        counties += "<span>"+county+"</span>";
      });
      ul.append($.nano('<li><a href="/pages/weather-alerts/?id={id}" title="Alert {index} of {totalItems}">{title} in effect for: {counties}</a></li>', {
           id: item.ID
           ,title: item.TITLE
           ,index: (i+1)
           ,totalItems: weather.length
           ,counties: counties
      }));
    });
    hasWeatherAlerts = true;
  }
  alerts.append(ul);

  breaking = [{"PERMALINK":"news\/two-correctional-officers-are-victims-of-assault","TITLE":"Two Correctional Officers are Victims of Assault"},{"PERMALINK":"news\/body-found-in-car-in-creek-after-man-child-reported-missing","TITLE":"Body Found in Car in Creek After Man, Child Reported Missing"},{"PERMALINK":"news\/blues-land-goalie-ryan-miller-in-blockbuster-trade","TITLE":"Blues Land Goalie Ryan Miller In Blockbuster Trade"},{"PERMALINK":"news\/mizzou-players-react-to-michael-sam-s-announcement","TITLE":"Mizzou Players React to Michael Sam's Announcement"},{"PERMALINK":"news\/mizzou-s-michael-sam-reveals-he-s-gay","TITLE":"Mizzou's Michael Sam Reveals He's Gay"}];
  
  breaking = 0;
  ul = $('<ul id="breaking_news_alerts"></ul>');
  if(breaking.length > 0){
    $.each(breaking, function(i, item){
      ul.append($.nano('<li><a href="/{permalink}/" title="{title}">{title}</a></li>', {
           title: item.TITLE
           ,permalink: item.PERMALINK
      }));
    });
    hasBreakingNewsAlerts = true;
  }
  alerts.append(ul);

  if(hasWeatherAlerts || hasBreakingNewsAlerts){
    if(hasWeatherAlerts && hasBreakingNewsAlerts){
      sections = [
          {id: 'breaking_news_alerts', title: 'Breaking News'}
          ,{id: 'weather_alerts', title: 'Weather Alerts'}
      ];
    }
    else if(hasWeatherAlerts){
      sections = [{id: 'weather_alerts', title: 'Weather Alerts'}];
    }
    else if(hasBreakingNewsAlerts){
      sections = [{id: 'breaking_news_alerts', title: 'Breaking News Alerts'}];
    }
    $('#alerts_ticker').append(alerts);
    $('#alerts_ticker').css('display', 'block').animate({top: 0}, 750, function(){
      t = $('#alerts_ticker_clip').ticker({
        sections: sections
        ,progressBarWidth: 186
      });				
      t.start();
      t.loadElement();
    });
  }
});
