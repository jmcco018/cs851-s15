    <script type="text/javascript">
        addthis.addEventListener('addthis.ready', function () {
            var Location = Sigalert.common.Location,
                Url = Chai.Url;
            
            function updateAddThis ()
            {
                addthis.update("share", "url", Url.addQueryString(Url.getLocation(addthis_share.url), "lat=" + Location.getLat() + "&lon=" + Location.getLon() + "&z=" + Location.getZoom()));
                addthis.update("share", "title", "Sigalert.com " + Location.getAreaDisplayName() + " Traffic Map");
            }
            
            if (Location.getRegion() !== null) {
                updateAddThis();
            }
            
            Location.onLatLonChanged.subscribe(updateAddThis);
        });
    </script>
