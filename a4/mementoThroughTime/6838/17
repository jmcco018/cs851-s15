            window.renderJobs = function() {
                $('.jobs-content').empty();
                // var num_cols = Math.floor((window.dynamic_width+28) / 328) - 2;
                var num_cols = 1;
                for(var i = 0; i < num_cols; i++) {
                    $('.jobs-content').append(
                        $.el.div({class: 'job'})
                    );
                }
                $('.jobs-content .job').each(function(i) {
                    $(this).html(jobTemplate.render(window.jobs[i % window.jobs.length]));
                });
            }

            window.renderSponsorMessages = function() {
                $('.sponsored-content').empty();
                var num_cols = 1;
                for(var i = 0; i < num_cols; i++) {
                    $('.sponsored-content').append(
                        $.el.div({class: 'sponsored-post'})
                    );
                }
                $('.sponsored-content .sponsored-post').each(function(i) {
                    $(this).html(sponsorPostTemplate.render(window.sponsor_messages[i % window.sponsor_messages.length]));
                });
            }

            window.rearrangeLeaderboards = function() {
                var newsCoversDiv = $('#newsroll .news-covers');
                var newsCovers = newsCoversDiv.find('.news-cover, .ad');
                var num_cols = Math.floor(($('#main-content .dynamic-width').width()+28) / 328);
                var num_items_in_3_rows = num_cols * 3;
                var num_items_in_3_rows_index = num_items_in_3_rows - 1;

                $('.news-cover.last-row').removeClass('last-row');
                if(num_cols >= 3 || ($(window).width() >= 768)) {
                    for (var i = num_items_in_3_rows_index; i <= newsCovers.length; i += num_items_in_3_rows) {

                        for(var ii = i; ii > i - num_cols; ii--) {
                            $(newsCovers.get(ii)).addClass('last-row');
                        }

                        var newsCover = $(newsCovers.get(i));
                        if (!newsCover.next('.ad-leaderboard').length) {
                            $($.el.div({class: 'ad-leaderboard dynamic-width rearrange-dirty'},
                                $.el.div({class: 'dynamic-width'}, 
                                    $.el.div({class: 'sponsored-content'}),
                                    // $.el.div({class: 'jobs'}, 
                                    //     $.el.div({class: 'masthead'},
                                    //         $.el.span({class: 'heading'}, 'Creative Jobs'),
                                    //         $.el.span({class: 'links'},
                                    //             $.el.a({href: '/web/20140515053307/http://jobs.thecreativefinder.com/'}, "View all"),
                                    //             " ",
                                    //             $.el.a({href: '/web/20140515053307/http://jobs.thecreativefinder.com/business-buy-job.php'}, "Post jobs")
                                    //         )
                                    //     ),
                                    //     // $.el.div({class: 'jobs-content'})
                                    // ),
                                    // $.el.iframe({scrolling: 'no', class: 'ad-728x90', seamless: 'seamless', src: 'ad-728x90.html'}),
                                    $.el.iframe({scrolling: 'no', class: 'ad-728x90', seamless: 'seamless', src: 'ad-728x90.html'}),
                                    $.el.div({class: 'advertise-here'},
                                        $.el.a({href: 'advertise.html'}, "Advertise here")
                                    )
                                )
                            )).insertAfter(newsCover);
                        } else {
                            newsCover.next('.ad-leaderboard').addClass('rearrange-dirty');
                        }
                    }
                }

                newsCoversDiv.find('.ad-leaderboard:not(.rearrange-dirty)').remove();
                newsCoversDiv.find('.ad-leaderboard.rearrange-dirty').removeClass('rearrange-dirty');
                newsCoversDiv.masonry('reload');

                window.renderSponsorMessages();
            };

            window.resizePage = function() {
                var window_width = $(window).width();

                if(window_width >= 1024) {
                    var padded_window_width = window_width - 40;
                    window.dynamic_width = padded_window_width - (padded_window_width % 328) - 28;
                    window.dynamic_cols = Math.floor((dynamic_width + 28) / 328);
                    jss('.dynamic-width', {
                        width: dynamic_width + 'px'
                    });

                    $('#large-screen-ads').empty();
                    $('#large-screen-ads2').empty();

                    if(dynamic_cols >= 4) {
                        $('#news').css('width', '878px');
                        $('#side-panel').show().width(dynamic_width - 984);
                        var largescreen_ad_amount = Math.floor((dynamic_width - 956 + 28) / 328);
                    } else {
                        $('#news').css('width', '550px');
                        $('#side-panel').show().width(dynamic_width - 656);
                        var largescreen_ad_amount = Math.floor((dynamic_width - 628 + 28) / 328);
                    }

                    // for(var i=0; i < largescreen_ad_amount; i++) {
                    //     $('#large-screen-ads').append(
                    //         $.el.div({class: 'ad'},
                    //             $.el.iframe({scrolling: 'no', class: 'ad-300x250', seamless: 'seamless', scrolling: 'no', src: 'ad-300x250.html'})
                    //         )
                    //     );
                    //     $('#large-screen-ads2').append(
                    //         $.el.div({class: 'ad'},
                    //             $.el.iframe({scrolling: 'no', class: 'ad-300x250', seamless: 'seamless', scrolling: 'no', src: '/web/20140515053307/http://bza.co/widget-dt-2.php'})
                    //         )
                    //     );
                    // }

                    for(var i=0; i < largescreen_ad_amount; i++) {
                        if(i == 0) {
                            $('#large-screen-ads').append(
                                $.el.div({class: 'ad'},
                                    $.el.iframe({seamless: 'seamless', scrolling: 'no', src: 'adplay/300x600.html', style: 'width: 300px; height: 600px;'})
                                )
                            );
                        } else {
                            $('#large-screen-ads').append(
                                $.el.div({class: 'ad'},
                                    $.el.iframe({scrolling: 'no', class: 'ad-300x250', style: 'margin-bottom: 50px;', seamless: 'seamless', scrolling: 'no', src: 'ad-300x250.html'}),
                                    $.el.iframe({scrolling: 'no', class: 'ad-300x250', style: 'margin-top: 50px;', seamless: 'seamless', scrolling: 'no', src: 'ad-300x250.html'})
                                )
                            );
                        }
                        
                        // $('#large-screen-ads2').append(
                        //     $.el.div({class: 'ad'},
                        //         $.el.iframe({scrolling: 'no', class: 'ad-300x250', seamless: 'seamless', scrolling: 'no', src: '/web/20140515053307/http://imgembed.com/imgembed-feature-images-widget.php'})
                        //     )
                        // );
                    }

                    var content_div = $('#similar-news .content');
                    content_div.empty();
                    content_div.masonry('reload');
                    var max_rows = Math.round(($('#news-content').height() - $('#side-panel').height())  / 270);
                    var num_news_covers_to_add = Math.min(largescreen_ad_amount * max_rows, window.related_news.length);
                    if (num_news_covers_to_add > 0) {
                        $('#similar-news .header').show();
                        for(var i=0; i < num_news_covers_to_add; i++) {
                            content_div.append(newsCoverTemplate.render(window.related_news[i]));
                        }
                    } else {
                        $('#similar-news .header').hide();
                    }
                    content_div.masonry('reload');
                } else {
                    $('#side-panel').hide();
                    $('#news').css('width', '');

                    jss('.dynamic-width', {
                        width: ''
                    });
                    $('#large-screen-ads').empty();
                }

                // if (dynamic_width) {
                //     jss('.leaderboard .jobs, .ad-leaderboard .jobs', {
                //         width: (dynamic_width - 5 - 730) + "px"
                //     });
                // }

                window.rearrangeLeaderboards();
            }

