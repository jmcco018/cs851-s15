        <script type="text/javascript">
            if(!isMobile.any()){
                $(function() {
                    var newsletter_popup_time = parseInt(Cookies('newsletter_popup'));
                    var time_now = Math.round(new Date().getTime() / 1000);
                    var next_visit_determinant = 86400; // 1 day

                    if(isNaN(newsletter_popup_time)) {
                        Cookies.set('newsletter_popup', time_now + next_visit_determinant);
                    } else if(newsletter_popup_time < 0) {
                        // Do not show
                    } else if(newsletter_popup_time < time_now) {
                        $('#newsletter-popup').reveal({
                             animation: 'fade', 
                             animationspeed: 300,
                             closeonbackgroundclick: true,
                             dismissmodalclass: 'close-reveal-modal'
                        });
                    }

                    $('#popup-newsletter-signup-form').submit(function() {
                        Cookies.set('newsletter_popup', -1);
                    });
                    $('#popup-newsletter-remind-later').click(function() {
                        Cookies.set('newsletter_popup', time_now + next_visit_determinant);
                        window.newsletter_popup_programmatic_close = true;
                        $('.close-reveal-modal').click();
                        return false;
                    });
                    $('.reveal-modal-bg, .close-reveal-modal').click(function() {
                        if(!window.newsletter_popup_programmatic_close) {
                            Cookies.set('newsletter_popup', -1);
                        }
                    });
                    $('#popup-newsletter-not-keen').click(function() {
                        $('.close-reveal-modal').click();
                        return false;
                    });
                });
            }
        </script>
