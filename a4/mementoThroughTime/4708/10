        var log = function(button_name) {
            return function() {
                var loggerArgs = {
                    'php_event_name': 'share_facebook',
                    'source': 'shop',
                    'button_name': button_name
                };
                if ("/web/20120408180659/http://www.facebook.com/ZigZagStitches".length > 0){
                    loggerArgs['post_to_page'] = '/web/20120408180659/http://www.facebook.com/ZigZagStitches';
