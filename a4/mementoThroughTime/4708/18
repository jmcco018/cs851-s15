                    <script type="text/javascript">
(function(Etsy) {

    Etsy.GA = Etsy.GA || {};

    // Cross platform function identification (Implementation from underscore.js)
    Etsy.GA.isFunction = function(obj) {
        return !!(obj && obj.constructor && obj.call && obj.apply);
    }

    // Wrapper method for Google Analytics tracking
    // This provides a common api for tracking calls in the sync and async versions
    // @param   array   first element should be method in GA to call
    //                  all other elements should be args to pass to that method
    Etsy.GA.track = function(args) {
        // Synchronous (older) version
        if (typeof window.pageTracker != 'undefined' && Etsy.GA.isFunction(window.pageTracker[args[0]])) {
            var method = args.shift();
            return window.pageTracker[method].apply(window.pageTracker,args);
        // Asynchronous (newer) version
        } else if (typeof window._gaq != 'undefined' && Etsy.GA.isFunction(window._gaq.push)) {
            window._gaq = window._gaq || [];
            return window._gaq.push(args);
        }

        return null;
    };

})(window.Etsy);
    window._gaq = window._gaq || [];
    Etsy.GA.track(['_setAccount', 'UA-2409779-1']);
    Etsy.GA.track(['_setCustomVar', 1, 'GA snippet', 'async']);
    (function() {
        var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
        ga.src = ('https:' == document.location.protocol ? '/web/20140605063609/https://ssl' : '/web/20140605063609/http://www') + '.google-analytics.com/ga.js';
        var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
    })();
</script>    
    
