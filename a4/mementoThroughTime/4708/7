     <script type="text/javascript" charset="utf-8">
        jQuery(function($) {
            function initializeLocaleOverlay(trigger) {
                Etsy.LocaleOverlay.init({
                    locale: {"language":{"code":"en-US","name":"English (US)","translation":"English (US)","is_detected":false,"is_default":true,"is_EU_lang":false,"is_EU_region":false},"currency":{"currency_id":840,"code":"USD","name":"United States Dollar","number_precision":2,"symbol":"$","listing_enabled":true,"browsing_enabled":true,"buyer_location_restricted":false,"rate_updates_enabled":true,"is_detected":false,"is_default":true},"region":{"code":"XX","name":"Everywhere","translation":"Everywhere","is_detected":false,"is_default":true}},
                    trigger: trigger
                });
                $("#locale-select-overlay").overlay({load:true});
                $('#footer-spinner').hide();
            }

            function loadAndInitializeLocaleOverlay(trigger) {
                if (Etsy.LocaleOverlay) {
                    // Locale js logic is ready
                    initializeLocaleOverlay(trigger);
                } else {
                    // Load locale js logic
                    Etsy.loader.require('overlays/locale');
                    Etsy.loader.complete(function() {
                        initializeLocaleOverlay(trigger);
                    });
                }
            }

            function getOverlaySourceURL() {
                return window.location.protocol == 'https:' ? "/locale/overlay_secure" : "/locale/overlay";
            }

            function showLocaleOverlay(e) {
                if ($("#locale_overlay_container").is(':empty')) {
                    $('#footer-spinner').css('display', 'inline-block');
                    $("#locale_overlay_container").load(getOverlaySourceURL(), null, function() {
                        loadAndInitializeLocaleOverlay(e.target);
                    });
                } else {
                    $("#locale-select-overlay").overlay({load:true});
                }
                pageTracker._trackEvent('Locale selector', 'footer');
            }

            $("a[href=#language-select],a[href=#region-select],a[href=#currency-select]").live('click', function(e) {
                showLocaleOverlay(e);
                e.preventDefault();
            });

        });
    </script>

